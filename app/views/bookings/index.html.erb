<div class="main-container">
  <div class="container">
    <div class="row ">
    <% @services.each do |service| %>
      <% service.bookings.each do |booking| %>
        <div class="col-xs-12">
          <div class="card-booking">
            <%= booking.user.first_name %>
            <%= booking.date %>
            <%= booking.service %>
          </div>
        </div>
      <% end %>
    <% end %>
    </div>
  </div>
  <div
    id="map"
    style="width: 100%;
    height: 600px;"
    data-markers="<%= @markers.to_json %>"
  ></div>
</div>
